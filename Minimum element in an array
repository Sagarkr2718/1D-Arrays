class Main {
	public static int minElement(int A[]){
		int min = A[0];
		for(int i = 1 ; i< A.length ; i++){
			if(A[i]<min){
				min = A[i];
			}
			
		}
		return min;
	}
	public static void main(String args[]) {
		// Your code goes here
		int A[] = { 10 , 4 , 2, 8, 4 ,20};
		int b = minElement(A);
		System.out.print(b);
	}
}
